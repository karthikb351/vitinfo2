        </activity>
        <activity
            android:name="com.karthikb351.vitinfo2.GetAttendance"
            android:label="Getting Attendance"
            android:screenOrientation="portrait"
            android:theme="@style/LightThemeSelector">
        </activity>
        <activity
            android:name="com.karthikb351.vitinfo2.SubjectDetails"
            android:label="Subject Details"
            android:screenOrientation="portrait"
